<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dudu.smartagriculture.dao.DeviceSymbolDao">
    <update id="updateDeviceSymbol"  parameterType="com.dudu.smartagriculture.mbg.model.DeviceSymbol" >
        UPDATE SET device_symbol(name,symbol)  VALUES(#{name},#{symbol})
        WHERE rule_name = #{rule_name}
    </update>
</mapper>